{
	"name": "GitGuard",
	"version": "1.1.1",
	"description": "Alters GitHub merge button style accordingly to the specified Jenkins jobs.",
	"manifest_version": 2,
	"options_page": "options.html",
	"background": {
		"scripts": ["gitguard.js"]
	},
	"icons" : {
		"48" : "icon_48.png",
		"128" : "icon_128.png"
    },
	"permissions": ["tabs", "storage", "http://*/*", "https://*/*"]
}